<div id="list-table">
	<table>
		<thead>
			<tr>
				<th>Caption</th>
				<th>Picture</th>
				<th colspan="3"></th>
			</tr>
		</thead>

		<tbody>
			<% @result.each do |post| %>
				<tr>
					<td><%= sanitize post["caption"] %></td>
					<td><%= image_tag post["image_permalink"], width: '50px' %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>



